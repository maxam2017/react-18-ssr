    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.5.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
